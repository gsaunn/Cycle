<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sandra.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/menstrual"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sandra.myapplication.MainActivity"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sandra.myapplication.Result"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_result"
            android:screenOrientation="portrait" >
        </activity>
        <receiver android:name="com.example.sandra.myapplication.MyBroadcastReceiver" />
        <receiver android:name="com.example.sandra.myapplication.MySecondBroadcast" />
    </application>

</manifest>
